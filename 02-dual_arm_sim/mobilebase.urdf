  <joint
    name="base_prismatic_x"
    type="prismatic">
    <origin
      xyz="0 0 0"
      rpy="0 0 0" />
    <parent
      link="floor" />
    <child
      link="base_x" />
    <axis
      xyz="1 0 0" />
    <limit lower="-200" upper="200" effort="1000" velocity="1000" />
  </joint>

  <joint
    name="base_prismatic_y"
    type="prismatic">
